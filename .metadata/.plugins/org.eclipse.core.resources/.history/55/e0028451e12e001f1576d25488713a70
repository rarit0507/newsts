<mapper namespace="boardMapper">
	
	
	<select id="boardCount" resultType="_int"> <!-- mybatis에 이렇게 쓰라고 써 있음 -->
		SELECT
				COUNT(BOARD_NO)
		  FROM
				BOARD
		 WHERE
				STATUS = 'Y'
	</select>
	
